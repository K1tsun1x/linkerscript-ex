{
  "name": "linkerscript-ex",
  "displayName": "LinkerScript?EX",
  "description": "Extension for LinkerScript (.ld)",
  "publisher": "K1tsun1x",
  "version": "1.0.0",
  "icon": "images/icon128x128.png",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other", "Programming Languages"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "keywords": [
    "linker",
    "linkerscript",
    "ld",
    "GNU ld"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/K1tsun1x/linkerscript-ex.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "linkerscript",
        "aliases": ["LinkerScript", "Linker Script", "linker script", "linkerscript", "ld"],
        "extensions": [".ld"],
        "icon": {
          "light": "images/icon128x128.png",
          "dark": "images/icon128x128.png"
        },
        "configuration": "language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "linkerscript",
        "scopeName": "source.linkerscript",
        "path": "syntaxes/linkerscript.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "linkerscript",
        "path": "snippets/linkerscript.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
